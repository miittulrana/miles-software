{
  "name": "miles-express-admin",
  "version": "1.0.0",
  "description": "Miles Express Admin Dashboard",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build": "webpack --mode production",
    "dev": "webpack --mode development && electron .",
    "watch": "webpack --mode development --watch",
    "dev:watch": "concurrently \"npm run watch\" \"electron .\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.milesexpress.admin",
    "productName": "Miles Express Admin",
    "files": [
      "src/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "mac": {
      "category": "public.app-category.business"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": ["deb", "rpm", "AppImage"]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.0",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "electron": "22.3.27",
    "electron-builder": "^24.6.3",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.31.0",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.5",
    "dotenv": "^16.3.1",
    "mapbox-gl": "^2.15.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2"
  }
}